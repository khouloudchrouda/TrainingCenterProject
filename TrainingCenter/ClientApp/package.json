    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/platform-server": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@fontsource/arima-madurai": "^5.0.5",
    "@swimlane/ngx-datatable": "^20.1.0",
    "@syncfusion/ej2-angular-buttons": "^23.1.42",
    "@syncfusion/ej2-angular-dropdowns": "23.1.42",
    "@syncfusion/ej2-buttons": "23.1.42",
    "bootstrap": "^5.3.2",
    "boxicons": "^2.1.4",
    "jquery": "^3.6.4",
    "ngx-quill": "^21.0.0",
    "oidc-client": "^1.11.5",
